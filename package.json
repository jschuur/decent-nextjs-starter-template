{
  "name": "decent-nextjs-starter-template",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently -c auto -t 'HH:mm:ss.SSS' -p '[{name} {time}]' bun:dev:*",
    "dev-nosst": "concurrently -c auto -t 'HH:mm:ss.SSS' -p '[{name} {time}]' bun:dev-next bun:dev:db:turso bun:dev:db:studio",
    "dev:next": "sst dev next dev",
    "dev-next": "next dev",
    "dev:db:studio": "drizzle-kit studio",
    "dev:db:turso": "turso dev --db-file ./db/dev.db",
    "build": "next build",
    "check": "tsc --noEmit",
    "lint": "next lint",
    "start": "next start",
    "deploy": "bun run deploy:preview",
    "deploy:preview": "sst deploy --stage preview",
    "deploy:prod": "sst deploy --stage production",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:migrate:preview": "dotenv -e .env.preview -- drizzle-kit migrate",
    "db:migrate:prod": "dotenv -e .env.production -- drizzle-kit migrate",
    "db:dump": "turso db shell http://127.0.0.1:8080 .dump e db/exports/backup_`date -u +\"%Y-%m-%dT%H-%M-%S\"`.sql",
    "db:dump:preview": "turso db shell decent-nextjs-starter-template-preview .dump > db/exports/backup_preview_`date -u +\"%Y-%m-%dT%H-%M-%S\"`.sql",
    "db:dump:prod": "turso db shell decent-nextjs-starter-template .dump > db/exports/backup_prod_`date -u +\"%Y-%m-%dT%H-%M-%S\"`.sql",
    "db:shell": "turso db shell http://127.0.0.1:8080",
    "db:shell:preview": "turso db shell decent-nextjs-starter-template-preview",
    "db:shell:prod": "turso db shell decent-nextjs-starter-template",
    "db:seed": "cat db/sample_data.sql | turso db shell http://127.0.0.1:8080",
    "db:seed:create": "turso db shell http://127.0.0.1:8080 .dump | grep -E '^(INSERT INTO stack_item|PRAGMA|BEGIN|COMMIT)' > db/sample_data.sql",
    "ui:add": "bunx --bun shadcn-ui@latest add"
  },
  "dependencies": {
    "@auth/drizzle-adapter": "^1.2.0",
    "@fontsource/space-grotesk": "^5.0.18",
    "@hookform/resolvers": "^3.6.0",
    "@libsql/client": "^0.6.2",
    "@next/third-parties": "^14.2.4",
    "@radix-ui/react-avatar": "^1.0.4",
    "@radix-ui/react-checkbox": "^1.0.4",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-popover": "^1.0.7",
    "@radix-ui/react-radio-group": "^1.1.3",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-switch": "^1.0.3",
    "@radix-ui/react-tabs": "^1.0.4",
    "@radix-ui/react-tooltip": "^1.0.7",
    "@t3-oss/env-nextjs": "^0.10.1",
    "@tabler/icons-react": "^3.5.0",
    "@tanstack/react-query": "^5.45.0",
    "@tanstack/react-query-devtools": "^5.45.0",
    "boolean": "^3.2.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "cmdk": "^1.0.0",
    "dotenv": "^16.4.5",
    "drizzle-orm": "^0.31.2",
    "drizzle-zod": "^0.5.1",
    "lucide-react": "^0.394.0",
    "next": "14.2.4",
    "next-auth": "5.0.0-beta.19",
    "next-themes": "^0.3.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.51.5",
    "react-spinners": "^0.13.8",
    "sonner": "^1.5.0",
    "sst": "^3.0.33",
    "tailwind-merge": "^2.3.0",
    "tailwindcss-animate": "^1.0.7",
    "usehooks-ts": "^3.1.0",
    "uuid": "^10.0.0",
    "zod": "^3.23.8",
    "zod-validation-error": "^3.3.0"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.10",
    "@types/node": "^20.14.2",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/uuid": "^9.0.8",
    "concurrently": "^8.2.2",
    "drizzle-kit": "^0.22.7",
    "eslint": "^8.57.0",
    "eslint-config-next": "14.2.4",
    "postcss": "^8.4.38",
    "tailwindcss": "^3.4.4",
    "typescript": "^5.4.5"
  }
}
