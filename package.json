{
  "name": "decent-nextjs-starter-template",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently -c auto -t 'HH:mm:ss.SSS' -p '[{name} {time}]' pnpm:dev:*",
    "dev-nosst": "concurrently -c auto -t 'HH:mm:ss.SSS' -p '[{name} {time}]' pnpm:dev-next pnpm:dev:db:turso pnpm:dev:db:studio",
    "dev:next": "sst dev next dev",
    "dev-next": "next dev",
    "dev:db:studio": "drizzle-kit studio",
    "dev:db:turso": "turso auth login && turso dev --db-file ./db/dev.db",
    "build": "next build",
    "check": "tsc --noEmit",
    "lint": "next lint",
    "format": "prettier --write .",
    "start": "next start",
    "deploy": "pnpm run deploy:preview",
    "deploy:preview": "dotenv -e .env.preview -- sst deploy --stage preview",
    "deploy:prod": "sst deploy --stage production",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:migrate:preview": "dotenv -e .env.preview -- drizzle-kit migrate",
    "db:migrate:prod": "dotenv -e .env.production -- drizzle-kit migrate",
    "db:dump": "turso db shell http://127.0.0.1:8080 .dump e db/exports/backup_`date -u +\"%Y-%m-%dT%H-%M-%S\"`.sql",
    "db:dump:preview": "turso db shell decent-nextjs-starter-template-preview .dump > db/exports/backup_preview_`date -u +\"%Y-%m-%dT%H-%M-%S\"`.sql",
    "db:dump:prod": "turso db shell decent-nextjs-starter-template .dump > db/exports/backup_prod_`date -u +\"%Y-%m-%dT%H-%M-%S\"`.sql",
    "db:shell": "turso db shell http://127.0.0.1:8080",
    "db:shell:preview": "turso db shell decent-nextjs-starter-template-preview",
    "db:shell:prod": "turso db shell decent-nextjs-starter-template",
    "db:seed": "cat db/sample_data.sql | turso db shell http://127.0.0.1:8080",
    "db:seed:prod": "cat db/sample_data.sql | turso db shell decent-nextjs-starter-template",
    "db:seed:create": "turso db shell http://127.0.0.1:8080 .dump | grep -E '^(INSERT INTO stack_item|PRAGMA|BEGIN|COMMIT)' > db/sample_data.sql",
    "db:seed:create:prod": "turso db shell decent-nextjs-starter-template .dump | grep -E '^(INSERT INTO stack_item|PRAGMA|BEGIN|COMMIT)' > db/sample_data.sql"
  },
  "dependencies": {
    "@auth/core": "^0.34.1",
    "@auth/drizzle-adapter": "^1.4.1",
    "@aws-sdk/client-cloudfront": "^3.609.0",
    "@dnd-kit/core": "^6.1.0",
    "@dnd-kit/modifiers": "^7.0.0",
    "@dnd-kit/sortable": "^8.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@faker-js/faker": "^8.4.1",
    "@fontsource/space-grotesk": "^5.0.18",
    "@hookform/resolvers": "^3.6.0",
    "@libsql/client": "^0.7.0",
    "@next/third-parties": "^14.2.4",
    "@radix-ui/react-alert-dialog": "^1.1.1",
    "@radix-ui/react-avatar": "^1.1.0",
    "@radix-ui/react-checkbox": "^1.1.1",
    "@radix-ui/react-dialog": "^1.1.1",
    "@radix-ui/react-dropdown-menu": "^2.1.1",
    "@radix-ui/react-label": "^2.1.0",
    "@radix-ui/react-popover": "^1.1.1",
    "@radix-ui/react-radio-group": "^1.2.0",
    "@radix-ui/react-select": "^2.1.1",
    "@radix-ui/react-separator": "^1.1.0",
    "@radix-ui/react-slot": "^1.1.0",
    "@radix-ui/react-switch": "^1.1.0",
    "@radix-ui/react-tabs": "^1.1.0",
    "@radix-ui/react-tooltip": "^1.1.2",
    "@sentry/nextjs": "^8.13.0",
    "@t3-oss/env-nextjs": "^0.10.1",
    "@tabler/icons-react": "^3.7.0",
    "@tanstack/react-query": "^5.49.2",
    "@tanstack/react-query-devtools": "^5.49.2",
    "@tanstack/react-table": "^8.19.2",
    "boolean": "^3.2.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "cmdk": "^1.0.0",
    "date-fns": "^3.6.0",
    "dotenv": "^16.4.5",
    "drizzle-orm": "^0.31.2",
    "drizzle-zod": "^0.5.1",
    "jotai": "^2.8.4",
    "lodash": "^4.17.21",
    "lucide-react": "^0.399.0",
    "next": "14.2.4",
    "next-auth": "5.0.0-beta.19",
    "next-themes": "^0.3.0",
    "pluralize": "^8.0.0",
    "pretty-ms": "^9.0.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.52.0",
    "react-hotkeys-hook": "^4.5.0",
    "react-spinners": "^0.14.1",
    "sonner": "^1.5.0",
    "sst": "^3.0.37",
    "tailwind-merge": "^2.3.0",
    "tailwindcss-animate": "^1.0.7",
    "usehooks-ts": "^3.1.0",
    "uuid": "^10.0.0",
    "zod": "^3.23.8",
    "zod-validation-error": "^3.3.0",
    "zustand": "^4.5.4"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.10",
    "@types/lodash": "^4.17.6",
    "@types/node": "^20.14.9",
    "@types/pluralize": "^0.0.33",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/uuid": "^10.0.0",
    "concurrently": "^8.2.2",
    "drizzle-kit": "^0.22.8",
    "eslint": "^8.57.0",
    "eslint-config-next": "14.2.4",
    "eslint-config-prettier": "^9.1.0",
    "postcss": "^8.4.39",
    "prettier-plugin-tailwindcss": "^0.6.5",
    "tailwindcss": "^3.4.4",
    "typescript": "5.5.3"
  }
}
